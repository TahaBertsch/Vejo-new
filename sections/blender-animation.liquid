{%- liquid
  assign video_url = section.settings.video_url
  assign image_overlay = section.settings.image
  assign shadow_amount = section.settings.overlay_shadow | divided_by: 100.00
  assign shadow_amount_lesser = shadow_amount | minus: 0.04
  assign overlay_opacity = section.settings.overlay_opacity | divided_by: 100.00
  assign align = section.settings.alignment | split: "_"
  assign enable_text_overlay = true
  assign video_aspect = section.settings.video_aspect
  assign disable_style = true

  if video_aspect == 'original' and video_url == blank
    assign disable_style = false
  endif

  if shadow_amount == 0
    assign enable_text_overlay = false
  endif

  assign brightness_button = block.settings.color_background_button | color_brightness
  if brightness_button > 60
    assign color_background_button_hover = block.settings.color_background_button | color_darken: 20
    else
    assign color_background_button_hover = block.settings.color_background_button | color_lighten: 20
  endif
  assign section_background_color = section.settings.gradient_background
-%}
{%- assign _section  = section.settings -%}
{% assign vejo_care_customer = false %}
{%- for tag in customer.tags -%}
  {%- if tag == "Vejo Care" or tag == "Vejo Invited" -%}
    {% assign vejo_care_customer = true %}
  {%- endif -%}
{%- endfor -%}

{{ "hp-blender-animation.scss.css" | asset_url | stylesheet_tag }}
<style>
  @media (max-width: 769px){
	.mobile_display_animation{
        display:block !important;
    }
  	.desktop_display_animation{
  		 display:none !important
   }
    .revers_display_animation{
  		 display:none !important
   }
  }
  .mobile_display_animation .image__img
    {
      position : inherit !important;
    }
  .mobile_display_animation .video-hero__video
    {
      position : inherit !important;
    }
  .revers_display_animation .image__img
    {
      position : inherit !important;
    }
  .revers_display_animation .video-hero__video
    {
      position : inherit !important;
    }
  .blender_animation_icon
    {
      fill:{{ _section.text_color }};
      text-indent: -2px !important;
     vertical-align: middle;
    }
  
</style>
<div class="sctn sctn-block-1 image-with-text--square" id="sctn-{{ section.id }}" style="background: {{ section_background_color }};">
  <div class="grid-overlay desktop_display_animation"  {%if section.settings.revers_asset =='right'%}style="display:none" {%endif%}>
    <div class="sctn-content">
      <div class="sctn-bg"></div>
      <div class="sctn-header desktop" style="color: {{ _section.text_color }}; padding:{{ section.settings.padding_content}}px;
                                      height: {{ section.settings.height_content}}px;justify-content:unset;text-align:{{ section.settings.text_alignment}}">
        <h2 style="margin-bottom:{{ section.settings.marign_bottom_content}}%; font-size:38px">{{ _section.main_headline }}</h2>
        <div  style="margin-bottom:{{ section.settings.marign_bottom_content}}%">{{ _section.sub_headline }}</div>
        {% if _section.show_bullets %}
        <ul style="list-style: none;">
          <li>{%- render 'icon' with icon:  section.settings.icon_bullet1 , class :'blender_animation_icon' -%} {{ _section.bullet_1 }}</li>
          <li>{%- render 'icon' with icon:  section.settings.icon_bullet2 , class :'blender_animation_icon' -%}{{ _section.bullet_2 }}</li>
          <li>{%- render 'icon' with icon:  section.settings.icon_bullet3 , class :'blender_animation_icon' -%}{{ _section.bullet_3 }}</li>
          <li>{%- render 'icon' with icon:  section.settings.icon_bullet4 , class :'blender_animation_icon' -%}{{ _section.bullet_4 }}</li>

        </ul>
        {% endif %}
       <div class="btn-wrp   " {% if section.settings.with_bottom == false %}style="display:none"{% endif %} >
              <a href="{{ section.settings.button_url}}"
                class="btn-link {% if section.settings.text_alignment == 'center' %}button_centered {% endif %}" 
                 style="border: {{section.settings.button_border_width}}px {{section.settings.button_border_style}} {{ section.settings.color_border_button}};
                   color:{{section.settings.button_text_color}}; background-color:{{section.settings.button_background_color}};border-radius: {{section.settings.button_border-radius}}px;"
              >{{ section.settings.button_text}}</a>
         </div>
      </div>
    </div>
    <div class="blender-scroll-animation" style="width:50%">
    {%- if video_url != blank -%}
      {%- if image_overlay != blank -%}
        {% render 'image' with image: image_overlay, disable_style: false, wrapper_class: 'video-hero__image' %}
      {%- endif -%}

      <video class="video-hero__video" autoplay playsinline loop muted src="{{ video_url }}"></video>
      {% render 'video-pause' %}
    {%- else -%}
      {%- if image_overlay != blank -%}
        {% render 'image' with image: image_overlay, disable_style: false, wrapper_class: 'video-hero__image' %}
         
    {%- endif -%}
    {%- endif -%}
    </div>
  </div>
  
  
   <div class="revers_display_animation"  {%if section.settings.revers_asset =='left'%}style="display:none" {%endif%}>
