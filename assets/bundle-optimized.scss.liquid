.post_article_header {
  background-color: #f7f7f7;
  height: 790px;
}
.article_header {
   float: right;
  
   width: 50%;
   padding: 3%;
}
.article_header h1 {
 margin-top: 42%;
}
.article_content {
  //vertical-align: bottom;
  margin-top: 52px;
}

@media (max-width: 769px){
  	.article_header{
        float: none !important;
    	width: 100% !important;
    	padding: 3% !important;

    }
	.article_header h1{
     	margin-top: 11% !important;
    }
  .post_article_header{
    height: 750px;
  }

}

.product_link a{
  background-color: #1d8c28;
  padding: 20px;
  border-radius: 35px;
  margin-bottom: 20px !important;
  color: white;
  width: 100% !important;
} 
mark {
 padding: 0px !important; 
}
.button_centered{
  	margin-left:auto !important;
  }
.withOut_button{
   		display:none;
    }
